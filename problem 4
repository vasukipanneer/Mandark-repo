   return (num3 >>> 24 & 0xFF) + '.' +
   (num3 >>> 16 & 0xFF) + '.' +
   (num3 >>> 8 & 0xFF) + '.' +
   (num3 & 0xFF);
};
document.write("C="+int32ToIp3(num3)+"<br>")
const num4 = 192255255255;
const int32ToIp4 = (num4) => {
   return (num4 >>> 24 & 0xFF) + '.' +
   (num4 >>> 16 & 0xFF) + '.' +
   (num4 >>> 8 & 0xFF) + '.' +
   (num4 & 0xFF);
};
document.write("D="+int32ToIp4(num4)+"<br>")
