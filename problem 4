const int32ToIp2 = (num2) => {
   return (num2 >>> 24 & 0xFF) + '.' +
   (num2 >>> 16 & 0xFF) + '.' +
   (num2 >>> 8 & 0xFF) + '.' +
   (num2 & 0xFF);
};
document.write("B="+int32ToIp(num)+"<br>")
